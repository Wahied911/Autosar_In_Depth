<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///C:/AutoSar%20Diploma/Lecture2/lab1/Autosar_ARXML_Schema/AUTOSAR_00046.xsd">	<AR-PACKAGES>		<AR-PACKAGE>			<!--  Define Main Package -->			<SHORT-NAME>SWCs_EABS</SHORT-NAME>			<!--  Define Elements -->			<ELEMENTS>				<!--1st Software Component is Lidar Sensor -->				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC1_Lidar_Sensor</SHORT-NAME>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>				<!--2nd Software Component is Speed Sensor -->				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC2_Speed_Sensor</SHORT-NAME>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>				<!--3rd Software Component is ECU Abstarct -->				<ECU-ABSTRACTION-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC3_ECU_Abstracrt</SHORT-NAME>				</ECU-ABSTRACTION-SW-COMPONENT-TYPE>				<!--4th Software Component is Application SWC for Algorithm -->				<APPLICATION-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC4_Application_Algorithm</SHORT-NAME>				</APPLICATION-SW-COMPONENT-TYPE>				<!--5th Software Component is Parameters SWC for Calibartion Data D1 , D2 & D3 -->				<PARAMETER-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC5_Distance_Parameters</SHORT-NAME>				</PARAMETER-SW-COMPONENT-TYPE>				<!--6th Software Component is Actuator for Wairning -->				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC6_Wairning_LED</SHORT-NAME>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>				<!--7th Software Component is Complex driver for Breaking (Time Critical) -->				<COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC7_Break</SHORT-NAME>				</COMPLEX-DEVICE-DRIVER-SW-COMPONENT-TYPE>				<!--8th Software Component is Service for interfacing with Diagnostics Manger -->				<SERVICE-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC8_Service_For_Diagnostics_Manger</SHORT-NAME>				</SERVICE-SW-COMPONENT-TYPE>				<!--8th Software Component is NVB to write on NVM direct -->				<NV-BLOCK-SW-COMPONENT-TYPE>					<SHORT-NAME>SWC9_NVM</SHORT-NAME>				</NV-BLOCK-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>