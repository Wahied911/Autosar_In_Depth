<?xml version="1.0" encoding="UTF-8"?><AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file:///C:/AutoSar%20Diploma/Lecture2/lab1/Autosar_ARXML_Schema/AUTOSAR_00046.xsd">	<!--Learn-In-Depth Ahmed Wahied -->	<AR-PACKAGES>		<AR-PACKAGE>			<SHORT-NAME>Lidar_System</SHORT-NAME>			<ELEMENTS>				<!-- Define Sender Reciver Interface -->				<SENDER-RECEIVER-INTERFACE>					<SHORT-NAME>SRI1</SHORT-NAME>					<DATA-ELEMENTS>						<!-- Define Variables -->						<!--Define Distance Variable   -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Lidar_Object_Distance</SHORT-NAME>							<SW-DATA-DEF-PROPS>								<SW-DATA-DEF-PROPS-VARIANTS>									<SW-DATA-DEF-PROPS-CONDITIONAL>										<SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>									</SW-DATA-DEF-PROPS-CONDITIONAL>								</SW-DATA-DEF-PROPS-VARIANTS>							</SW-DATA-DEF-PROPS>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>10</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>						<!--Define Object ID Variable   -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Lidar_Object_ID</SHORT-NAME>							<SW-DATA-DEF-PROPS>								<SW-DATA-DEF-PROPS-VARIANTS>									<SW-DATA-DEF-PROPS-CONDITIONAL>										<SW-IMPL-POLICY>QUEUED</SW-IMPL-POLICY>									</SW-DATA-DEF-PROPS-CONDITIONAL>								</SW-DATA-DEF-PROPS-VARIANTS>							</SW-DATA-DEF-PROPS>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>11</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>						<!--Define Object angle Variable   -->						<VARIABLE-DATA-PROTOTYPE>							<SHORT-NAME>Lidar_Object_Angle</SHORT-NAME>							<SW-DATA-DEF-PROPS>								<SW-DATA-DEF-PROPS-VARIANTS>									<SW-DATA-DEF-PROPS-CONDITIONAL>										<SW-IMPL-POLICY>STANDARD</SW-IMPL-POLICY>									</SW-DATA-DEF-PROPS-CONDITIONAL>								</SW-DATA-DEF-PROPS-VARIANTS>							</SW-DATA-DEF-PROPS>							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>							<INIT-VALUE>								<NUMERICAL-VALUE-SPECIFICATION>									<VALUE>13</VALUE>								</NUMERICAL-VALUE-SPECIFICATION>							</INIT-VALUE>						</VARIABLE-DATA-PROTOTYPE>					</DATA-ELEMENTS>				</SENDER-RECEIVER-INTERFACE>				<!-- Define SWCs -->				<!-- define Provided SWC as APPLICATION-SW-COMPONENT-TYPE -->				<APPLICATION-SW-COMPONENT-TYPE>					<SHORT-NAME>Lidar_SWC</SHORT-NAME>					<PORTS>						<P-PORT-PROTOTYPE>							<SHORT-NAME>Provider_Port</SHORT-NAME>							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">Lidar_System/SRI1</PROVIDED-INTERFACE-TREF>						</P-PORT-PROTOTYPE>						<!-- Define Port as Provided port -->					</PORTS>				</APPLICATION-SW-COMPONENT-TYPE>				<!-- define Recived SWC as SENSOR-ACTUATOR-SW-COMPONENT-TYP -->				<SENSOR-ACTUATOR-SW-COMPONENT-TYPE>					<SHORT-NAME>DashBoard</SHORT-NAME>					<PORTS>						<R-PORT-PROTOTYPE>							<SHORT-NAME>Recived_port</SHORT-NAME>							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">Lidar_System/SRI1</REQUIRED-INTERFACE-TREF>						</R-PORT-PROTOTYPE>					</PORTS>				</SENSOR-ACTUATOR-SW-COMPONENT-TYPE>			</ELEMENTS>		</AR-PACKAGE>	</AR-PACKAGES></AUTOSAR>